<!DOCTYPE html>
<html>
<head>
  <title><%= content_for?(:title) ? yield(:title) : "Youtu" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="header">
<%= render "shared/banner"%>
</div>
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <%#= bootstrap_flash %>
      <%= yield %>
 </div>
</div>

<div class="foot">
<%= render "shared/foot"%>
</div>
</div>
</body>
</html>
<style>
body {background-color: #DBD9DB;}
</style>
